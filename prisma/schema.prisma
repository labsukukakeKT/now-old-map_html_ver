// データベース接続設定 (Supabaseなど)
datasource db {
  provider = "postgresql" // SupabaseはPostgreSQL
  url      = env("DATABASE_URL") // .envファイルから接続URLを読み込む
}

// Prisma Client生成設定
generator client {
  provider = "prisma-client-js"
}

model spot_locations {
  // 1. カラム名
  // 2. データ型 (Prismaの型)
  // 3. アトリビュート (追加の属性や制約)
  lat      Int 
  lng      Int     
  name     String   @id                           // プライマリキー
  abst     String
  detail   String
}
